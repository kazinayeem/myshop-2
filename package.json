    "client": "npm run dev --prefix admin-panel",
    "seed": "node ./src/seed/seed.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run client\""
  },
  "keywords": [
    "node",
    "express",
    "mongodb",
    "mongoose",
    "bcryptjs",
    "jsonwebtoken",
    "multer",
    "sharp"
