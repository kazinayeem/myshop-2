  "author": "Mohammad Ali Nayeem",
  "description": "A simple Node.js application with Express, MongoDB, Mongoose, Bcryptjs, Jsonwebtoken, Multer, and Sharp.",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@faker-js/faker": "^9.5.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.10.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.5",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4",
    "zod": "^3.24.2"
